@model IEnumerable<NavbarParent>

<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false" asp-controller="Tutorial" asp-action="Index">
        Dynamic Dropdown
    </a>
    <ul class="dropdown-menu" aria-labelledby="navbarDarkDropdownMenuLink">
        @foreach (var nav in Model)
        {
                <li><a class="dropdown-item" asp-Controller="@nav.ParentAspControl" asp-action="@nav.ParentAspAction" asp-route-id="@nav.Id">@nav.ParentNavbarName</a></li>
        }
    </ul>
</li>